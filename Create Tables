  CREATE TABLE "AVAILABILITY" 
   (	"ID" NUMBER GENERATED BY DEFAULT ON NULL AS IDENTITY MINVALUE 1 MAXVALUE 9999999999999999999999999999 INCREMENT BY 1 START WITH 1 CACHE 20 NOORDER  NOCYCLE  NOKEEP  NOSCALE  NOT NULL ENABLE, 
	"NAME" NVARCHAR2(20) NOT NULL ENABLE, 
	 CONSTRAINT "AVAILABILITY_PK" PRIMARY KEY ("ID")
  USING INDEX  ENABLE
   ) ;

  CREATE TABLE "CUSTOMERS" 
   (	"ID" NUMBER NOT NULL ENABLE, 
	"NAME" NVARCHAR2(50) NOT NULL ENABLE, 
	"EMAIL" NVARCHAR2(50) NOT NULL ENABLE, 
	"PHONE" NVARCHAR2(10) NOT NULL ENABLE, 
	 CONSTRAINT "CUSTOMERS_CON" PRIMARY KEY ("ID")
  USING INDEX  ENABLE
   ) ;

  CREATE TABLE "FOOTBALLEVENTSSALES" 
   (	"SALE_ID" NUMBER NOT NULL ENABLE, 
	"CUSTOMER_ID" NUMBER NOT NULL ENABLE, 
	"TICKET_ID" NUMBER NOT NULL ENABLE, 
	"MATCH_ID" NUMBER NOT NULL ENABLE, 
	"SALE_DATE" DATE NOT NULL ENABLE, 
	 CONSTRAINT "FOOTBALLEVENTSSALES_CON" PRIMARY KEY ("SALE_ID")
  USING INDEX  ENABLE
   ) ;

  ALTER TABLE "FOOTBALLEVENTSSALES" ADD CONSTRAINT "FOOTBALLEVENTSSALES_CUST" FOREIGN KEY ("CUSTOMER_ID")
	  REFERENCES "CUSTOMERS" ("ID") ENABLE;
  ALTER TABLE "FOOTBALLEVENTSSALES" ADD CONSTRAINT "FOOTBALLEVENTSSALES_MATCH" FOREIGN KEY ("MATCH_ID")
	  REFERENCES "FOOTBALLMATCHES" ("MATCH_ID") ENABLE;
  ALTER TABLE "FOOTBALLEVENTSSALES" ADD CONSTRAINT "FOOTBALLEVENTSSALES_TICKET" FOREIGN KEY ("TICKET_ID")
	  REFERENCES "TICKETS" ("TICKET_ID") ENABLE;

  CREATE INDEX "FOOTBALLEVENTSSALES_IDX" ON "FOOTBALLEVENTSSALES" ("CUSTOMER_ID") 
  ;

  CREATE INDEX "FOOTBALLEVENTSSALES_IDX_1" ON "FOOTBALLEVENTSSALES" ("TICKET_ID") 
  ;

  CREATE INDEX "FOOTBALLEVENTSSALES_IDX_2" ON "FOOTBALLEVENTSSALES" ("MATCH_ID") 
  ;

  CREATE TABLE "FOOTBALLMATCHES" 
   (	"MATCH_ID" NUMBER NOT NULL ENABLE, 
	"NAME" NVARCHAR2(20) NOT NULL ENABLE, 
	"DATE" DATE NOT NULL ENABLE, 
	"STADIUM" NVARCHAR2(50) NOT NULL ENABLE, 
	 CONSTRAINT "FOOTBALLMATCHES_CON" PRIMARY KEY ("MATCH_ID")
  USING INDEX  ENABLE
   ) ;

  CREATE TABLE "TICKETS" 
   (	"TICKET_ID" NUMBER NOT NULL ENABLE, 
	"TICKET_TYPE" NVARCHAR2(10) NOT NULL ENABLE, 
	"PRICE" NUMBER NOT NULL ENABLE, 
	"AVAILIBILITYS" NUMBER NOT NULL ENABLE, 
	"NUMBER_TICKETS" NUMBER, 
	 CONSTRAINT "TICKETS_CON" PRIMARY KEY ("TICKET_ID")
  USING INDEX  ENABLE
   ) ;

  ALTER TABLE "TICKETS" ADD CONSTRAINT "TICKETS_CON1" FOREIGN KEY ("AVAILIBILITYS")
	  REFERENCES "AVAILABILITY" ("ID") ENABLE;

  CREATE INDEX "TICKETS_IDX" ON "TICKETS" ("NUMBER_TICKETS") 
  ;
